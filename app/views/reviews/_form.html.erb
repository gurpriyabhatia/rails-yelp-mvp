<%= form_for [@restaurant, @review] do |f| %>
  <div class="form-group">
    <%= f.label :content %>
    <%= f.text_field :"content", class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= f.label :rating %>
    <%= f.select :rating, options_for_select([1,2,3,4,5]), { }, {class: 'form-control'} %>
  </div>

  <%= f.submit class: 'btn btn-primary' %>
<% end %>
